  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "install:playwright": "playwright install --with-deps",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxi analyze",
    "test": "vitest",
    "coverage": "vitest run --coverage --coverage.include='**' --coverage.include='!stories' --coverage.include='!storybook-static' --coverage.include='!**/.nuxt' --coverage.include='!**/*.config.ts'",
    "test:nitro": "vitest --config tests/server/nitro/vitest.config.ts",
    "test:nuxt": "vitest --config tests/client/nuxt/vitest.config.ts",
    "test:nuxt:coverage": "pnpm test:nuxt --coverage",
    "test:node": "vitest --config tests/server/node/vitest.config.ts",
    "test:vue": "vitest --config tests/client/vue/vitest.config.ts",
    "test:e2e": "PLAYWRIGHT_HTML_OUTPUT_DIR=tests/e2e/playwright-report playwright test --config tests/e2e/playwright.config.ts",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "commitlint": "commitlint --edit",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build"
  },
